def main(field[2] P1, field[2] P2, field[6] params) -> (field[2]):
 	//https://en.wikipedia.org/wiki/Twisted_Edwards_curve#Addition_on_twisted_Edwards_curves

    field d = params[0] 
    field a = params[1]

   	field u = P1[0]
   	field v = P1[1]
   	field x = P2[0]
   	field y = P2[1]

 	field du = (u*y + x*v) / (1 + d * u*v*x*y)
  	field dv = (v*y - a * u*x)/(1 - d * u*v*x*y)

	return [du, dv]